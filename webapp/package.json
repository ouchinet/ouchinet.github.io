{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ouchinet.github.io",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.2.1",
    "cra-template": "1.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "npx react-scripts start --use-npm",
    "build": "npx react-scripts build --use-npm",
    "test": "npx react-scripts test --use-npm",
    "eject": "npx react-scripts eject --use-npm",

    "rm": "rm -rf main",
    "mv": "mv build main",
    "git": "git add . & git commit & git push origin code-react",
    "deploy": "npm run rm & npm run build & npm run mv & npm run git"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
